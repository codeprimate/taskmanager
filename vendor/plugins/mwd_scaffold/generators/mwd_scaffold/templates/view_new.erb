<h2>New <%= singular_name.titleize %></h2>

<ul class="linkbar">
	<%%= link_to 'Back', collection_url, :wrap_in=>'li' %>
</ul>

<%%= error_messages_for :<%= singular_name %> %>

<%% form_for(:<%= singular_name %>, :url => collection_url) do |f| %>
<fieldset>
	<legend><%= singular_name.titleize %></legend>
  	<%%= render :partial => "form", :locals => { :f => f } %>
  	<p>
    	<%%= submit_tag "Create", :class=>'submit' %>
  	</p>
</fieldset>
<%% end %>
