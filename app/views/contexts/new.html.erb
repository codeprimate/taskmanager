<h1>New Context</h1>

<%= error_messages_for :context %>

<% form_for(:context, :url => collection_url) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%= submit_tag "Create" %>
    <%= link_to "Cancel", :back %>
  </p>
<% end %>

<script type="text/javascript">
    $('context_name').focus();
</script>