<h1>New Project</h1>

<%= error_messages_for :project %>

<% form_for(:project, :url => collection_url) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%= submit_tag "Create" %>
    <%= link_to("Cancel", :back) %>
  </p>
<% end %>

<script type="text/javascript">
    $('project_name').focus();
</script>