<h1>Listing Projects</h1>

<table>
  <tr>
  </tr>
  <%- @projects.each do |project|%>
    <tr>
    
      <td><%=link_to 'Show', object_url(project) %></td>
      <td><%=link_to 'Edit', edit_object_url(project) %></td>
      <td><%=link_to 'Destroy', object_url(project), :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>
<br/>

